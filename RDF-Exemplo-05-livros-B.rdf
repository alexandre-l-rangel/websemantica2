         xmlns:ex="http://alexandre-l-rangel.github.io/websemantica2#">

  <!-- Definindo a classe Livro usando RDFS -->
  <rdf:Description rdf:about="http://alexandre-l-rangel.github.io/websemantica2#Livro">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Livro</rdfs:label>
    <rdfs:comment>Classe que representa um livro</rdfs:comment>
  </rdf:Description>

  <!-- Definindo a classe Autor usando RDFS -->
  <rdf:Description rdf:about="http://alexandre-l-rangel.github.io/websemantica2#Autor">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Autor</rdfs:label>
    <rdfs:comment>Classe que representa um autor de livros</rdfs:comment>
  </rdf:Description>

  <!-- Definindo a propriedade "escritoPor" que conecta Livro a Autor -->
  <rdf:Description rdf:about="http://alexandre-l-rangel.github.io/websemantica2#escritoPor">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label>escritoPor</rdfs:label>
    <rdfs:domain rdf:resource="http://alexandre-l-rangel.github.io/websemantica2#Livro"/>
    <rdfs:range rdf:resource="http://alexandre-l-rangel.github.io/websemantica2#Autor"/>
    <rdfs:comment>Relaciona um livro ao seu autor</rdfs:comment>
  </rdf:Description>

  <!-- Instância de Livro: O Senhor dos Anéis -->
